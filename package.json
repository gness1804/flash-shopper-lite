{
  "name": "svelte-app",
  "version": "1.0.1",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "mini-css-extract-plugin": "^0.6.0",
    "prettier": "1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "build:fullsize": "node ./scripts/build.js --nogzip",
    "dev": "node ./scripts/dev.js",
    "dev:open": "node ./scripts/dev.js --open",
    "start": "node ./scripts/start.js",
    "start:open": "node ./scripts/start.js --open",
    "lint": "node_modules/.bin/eslint --ext .js,.svelte  src scripts",
    "reset": "npm prune && npm install",
    "e2e:open": "cypress open",
    "e2e": "cypress run",
    "prettier": "prettier --write --plugin-search-dir=. ./**/*.svelte ./**/*.html ./**/*.js ./**/*.spec.js preCommit.js",
    "deploy": "now",
    "version": "node ./scripts/version.js",
    "version:minor": "node ./scripts/version.js --minor",
    "version:major": "node ./scripts/version.js --major"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "9.0.0",
    "commander": "^3.0.2",
    "http-serve": "^1.0.1",
    "js-cookie": "^2.2.1",
    "postcss": "^7.0.18",
    "postcss-import": "^12.0.1",
    "precompress": "^5.1.0",
    "semver": "^6.3.0",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "svelte-preprocess": "^3.1.1",
    "tailwindcss": "^1.1.2"
  }
